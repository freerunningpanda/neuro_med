    "version": "0.2.0",
    "configurations": [
        {
            "name": "All tests",
            "request": "launch",
            "type": "dart",
            "program": "./test/",
        },
        {
            "name": "upmind_dev",
            "program": "lib/main_dev.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
            ]
        },
        {
            "name": "upmind",
            "program": "lib/main.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "prod",
            ]
        },
        {
            "name": "upmind_no_flavor",
            "program": "lib/main_dev.dart",
            "request": "launch",
            "type": "dart",
        },
        {
            "name": "upmind-front-client (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "upmind-front-client (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}